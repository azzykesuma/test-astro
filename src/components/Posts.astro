---
import '../styles/global.css';
const postLists = await fetch('https://jsonplaceholder.typicode.com/posts').then((res) => res.json());
---

<div id="container">
	<main>
		<section id="hero" class="p-4">
			<h1>Posts</h1>
			{postLists.map((post) => <p>{post.title}</p>)}
		</section>
	</main>
</div>
